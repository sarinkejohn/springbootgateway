{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "service": { "type": "string" },
    "name": { "type": "string" },
    "amount": { "type": "string", "pattern": "^[0-9,]+$" },
    "account": { "type": "string" },
    "reference": { "type": "string" }
  },
  "required": ["service", "name", "amount", "account", "reference"],
  "additionalProperties": false
}
